{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "bundler",
        "lib": [
            "esnext"
        ],
        "types": [
            "@cloudflare/workers-types"
        ],
        "strict": true,
        "skipLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "../src/*"
            ]
        }
    },
    "include": [
        "**/*.ts",
        "../src/lib/types.ts"
    ]
}